{
  "AppSettings": {
    "Secret": "this is a very long string to be used as secret",
    "SmtpHost": "smtp.host",
    "SmtpPort": 587,
    "SmtpUsername": "username@mysite.com",
    "SmtpPassword": "passssss"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=localhost\\SQLEXPRESS;Initial Catalog=my-eshop-DB;Integrated Security=SSPI;",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ],
    "Enrich": [ "WithClientIp", "WithClientAgent" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=localhost\\SQLEXPRESS;Initial Catalog=my-eshop-DB;Integrated Security=SSPI;"
  }
}
